{
  "_id" : "_design/achievement",
  "views" : {
    "byGameId" : {
      "map" : "function(doc){ emit(doc.gameId, doc)}"
    },
    "byGameIdAndName" : {
        "map" : "function(doc) { emit([doc.gameId, doc.name], doc)}"
    },
    "nameByOwnerIdAndGameId": {
        "map" : "function(doc) { emit([doc.ownerId, doc.gameId], doc.name)}"
    }
  }
}
